    "$MARMALADE_JUICE_ROOT/Components/Profiler/Profiler"
}

librarypath "$MARMALADE_JUICE_ROOT/Components/Profiler/lib/arm"
library Profiler

deployments
{
    assets="assets"
    app-icf="data/profiler_app.icf"
}
